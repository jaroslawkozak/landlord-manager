#%RAML 1.0
title: lm-data-service API
baseUri:
version: v0.1

/property
	get:
		queryParameters:
			access_token:
	/{propertyId}:
		get:
			description: Retrieve a specific property details
			queryParameters:
				access_token:
			responses:
				200:
					body:
						application/json:
							example: | 
								{
									"propertyData" : {
										"id" : "1"
										"name" : "default property name",
										"type" : "property type",
										"address" : {
											"country" : "poland",
											"city" : "sosnowiec",
											"street" : "3 maja",
											"houseNumber" : "13/4",
											"postalCode" : "41-200"
										},
										"forRental" : true
									}
									"status" : "ok"
								}
				204:
					body:
						application/json:
							example: | 
								{
									"status" : "property doesn't exist"
								}
		put:
			queryParameters:
				access_token:

		delete:
			queryParameters:
				access_token:

/agreement
